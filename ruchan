```lua
-- Create the Screen GUI
local player = game.Players.LocalPlayer
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "ruchan"
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Create a Frame for the dark theme
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0.3, 0, 0.5, 0)
frame.Position = UDim2.new(0.35, 0, 0.25, 0)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0
frame.Parent = screenGui

-- Create a TextLabel for the title
local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(1, 0, 0.2, 0)
titleLabel.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
titleLabel.Text = "Ruchan GUI"
titleLabel.Parent = frame

-- Function to change Speed
local function changeSpeed()
    local newSpeed = player.Character.Humanoid.WalkSpeed == 16 and 50 or 16
    player.Character.Humanoid.WalkSpeed = newSpeed
end

-- Create a Button for changing Speed
local speedButton = Instance.new("TextButton")
speedButton.Size = UDim2.new(1, 0, 0.2, 0)
speedButton.Position = UDim2.new(0, 0, 0.2, 0)
speedButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
speedButton.TextColor3 = Color3.fromRGB(255, 255, 255)
speedButton.Text = "Toggle Speed"
speedButton.MouseButton1Click:Connect(changeSpeed)
speedButton.Parent = frame

-- Function to change Jump Power
local function changeJump()
    local newJumpPower = player.Character.Humanoid.JumpPower == 50 and 100 or 50
    player.Character.Humanoid.JumpPower = newJumpPower
end

-- Create a Button for changing Jump Power
local jumpButton = Instance.new("TextButton")
jumpButton.Size = UDim2.new(1, 0, 0.2, 0)
jumpButton.Position = UDim2.new(0, 0, 0.4, 0)
jumpButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
jumpButton.TextColor3 = Color3.fromRGB(255, 255, 255)
jumpButton.Text = "Toggle Jump Power"
jumpButton.MouseButton1Click:Connect(changeJump)
jumpButton.Parent = frame

-- Function to enable ESP
local function toggleESP()
    for _, obj in pairs(workspace:GetChildren()) do
        if obj:IsA("Player") then
            local highlight = Instance.new("Highlight")
            highlight.Parent = obj
            highlight.FillTransparency = 0.5
            highlight.OutlineColor = Color3.new(1, 0, 0)
        end
    end
end

-- Create a Button for the ESP
local espButton = Instance.new("TextButton")
espButton.Size = UDim2.new(1, 0, 0.2, 0)
espButton.Position = UDim2.new(0, 0, 0.6, 0)
espButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
espButton.TextColor3 = Color3.fromRGB(255, 255, 255)
espButton.Text = "Toggle ESP"
espButton.MouseButton1Click:Connect(toggleESP)
espButton.Parent = frame

-- Function to teleport to player
local function teleportToPlayer()
    local playerName = "PlayerNameHere" -- Change this to the desired player's name
    local targetPlayer = game.Players:FindFirstChild(playerName)
    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
    end
end

-- Create a Button for teleport to player
local teleportButton = Instance.new("TextButton")
teleportButton.Size = UDim2.new(1, 0, 0.2, 0)
teleportButton.Position = UDim2.new(0, 0, 0.8, 0)
teleportButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
teleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
teleportButton.Text = "Teleport to Player"
teleportButton.MouseButton1Click:Connect(teleportToPlayer)
teleportButton.Parent = frame
```
